// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Character {
  id      Int    @id @default(autoincrement())
  kanji   String?
  onyomi  String?
  kunyomi String?
  jlpt    String?
  meaning String?
}